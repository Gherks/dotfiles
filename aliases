# Unix
alias d="dirs -v"
alias grep='grep --color=auto --exclude-dir={.bzr,CVS,.git,.hg,.svn}'
alias l="ls -lah"
alias ll="ls -al"
alias ln="ln -v"
alias ls="ls --color=tty"
alias mkdir="mkdir -p"
alias e="$EDITOR"
alias v="$VISUAL"

# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local
